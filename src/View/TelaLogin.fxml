<AnchorPane id="AnchorPane" prefHeight="184.0" prefWidth="233.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.TelaLoginController">
   <children>
      <ImageView fitHeight="194.0" fitWidth="304.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/background.jpg" />
         </image>
      </ImageView>
      <TextField fx:id="txEmail" layoutX="97.0" layoutY="45.0" promptText="Login" />
      <PasswordField fx:id="txSenha" layoutX="97.0" layoutY="85.0" promptText="Senha" />
      <Button fx:id="btEntrar" layoutX="78.0" layoutY="126.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="80.0" text="Entrar">
         <font>
            <Font name="Copperplate Gothic Bold" size="14.0" />
         </font></Button>
      <Button fx:id="btSair" layoutX="181.0" layoutY="126.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="68.0" text="Sair">
         <font>
            <Font name="Copperplate Gothic Bold" size="14.0" />
         </font></Button>
      <Label layoutX="38.0" layoutY="49.0" text="LOGIN" textFill="WHITE">
         <font>
            <Font name="Copperplate Gothic Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="34.0" layoutY="90.0" text="SENHA" textFill="WHITE">
         <font>
            <Font name="Copperplate Gothic Bold" size="14.0" />
